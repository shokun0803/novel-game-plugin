{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "globals": {
    "wp": "readonly",
    "jQuery": "readonly",
    "novelGameFront": "readonly",
    "novelGameMeta": "readonly",
    "novelGameScenes": "readonly",
    "novelGameFlagData": "readonly",
    "noveltoolExportImport": "readonly",
    "debugLog": "readonly",
    "window": "readonly"
  },
  "rules": {
    "no-console": "error"
  },
  "overrides": [
    {
      "files": ["js/debug-log.js"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
